<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="ru.perm.medicalservices.ordercrud.Entity.StatusEntity" table="Status" schema="public"
           catalog="postgres">
        <id name="idStatus">
            <column name="ID_Status" sql-type="integer"/>
        </id>
        <property name="sName">
            <column name="S_Name" sql-type="text"/>
        </property>
        <set name="requestsByIdStatus" inverse="true">
            <key>
                <column name="ID_Status"/>
            </key>
            <one-to-many not-found="ignore" class="ru.perm.medicalservices.ordercrud.Entity.RequestEntity"/>
        </set>
    </class>
</hibernate-mapping>