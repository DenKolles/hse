<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="ru.perm.medicalservices.ordercrud.Entity.RequestEntity" table="Request" schema="public"
           catalog="postgres">
        <id name="idRequest">
            <column name="ID_Request" sql-type="integer"/>
        </id>
        <property name="rName">
            <column name="R_Name" sql-type="text"/>
        </property>
        <property name="rBeginDate">
            <column name="R_Begin_Date" sql-type="date"/>
        </property>
        <property name="rEndDate">
            <column name="R_End_Date" sql-type="date" not-null="true"/>
        </property>
        <property name="rUpdateDate">
            <column name="R_Update_Date" sql-type="date"/>
        </property>
        <many-to-one name="statusByIdStatus" class="ru.perm.medicalservices.ordercrud.Entity.StatusEntity">
            <column name="ID_Status"/>
        </many-to-one>
        <many-to-one name="patientByIdPatient" class="ru.perm.medicalservices.ordercrud.Entity.PatientEntity">
            <column name="ID_Patient" not-null="true"/>
        </many-to-one>
        <set name="actsByIdRequest" inverse="true">
            <key>
                <column name="ID_Request" not-null="true"/>
            </key>
            <one-to-many not-found="ignore" class="ru.perm.medicalservices.ordercrud.Entity.ActEntity"/>
        </set>
        <set name="referralsByIdRequest" inverse="true">
            <key>
                <column name="ID_Request" not-null="true"/>
            </key>
            <one-to-many not-found="ignore" class="ru.perm.medicalservices.ordercrud.Entity.ReferralEntity"/>
        </set>
    </class>
</hibernate-mapping>