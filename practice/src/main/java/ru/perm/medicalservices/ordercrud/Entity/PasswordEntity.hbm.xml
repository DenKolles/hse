<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="ru.perm.medicalservices.ordercrud.Entity.PasswordEntity" table="Password" schema="public"
           catalog="postgres">
        <id name="idPassword">
            <column name="ID_Password" sql-type="bigint"/>
        </id>
        <property name="pValue">
            <column name="P_Value" sql-type="text"/>
        </property>
        <set name="employeesByIdPassword" inverse="true">
            <key>
                <column name="ID_Password" not-null="true"/>
            </key>
            <one-to-many not-found="ignore" class="ru.perm.medicalservices.ordercrud.Entity.EmployeeEntity"/>
        </set>
    </class>
</hibernate-mapping>