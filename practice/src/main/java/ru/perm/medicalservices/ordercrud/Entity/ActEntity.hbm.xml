<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="ru.perm.medicalservices.ordercrud.Entity.ActEntity" table="Act" schema="public" catalog="postgres">
        <id name="idAct">
            <column name="ID_Act" sql-type="integer"/>
        </id>
        <property name="aName">
            <column name="A_Name" sql-type="text"/>
        </property>
        <property name="aDate">
            <column name="A_Date" sql-type="date"/>
        </property>
        <many-to-one name="requestByIdRequest" class="ru.perm.medicalservices.ordercrud.Entity.RequestEntity">
            <column name="ID_Request" not-null="true"/>
        </many-to-one>
        <many-to-one name="employeeByELogin" class="ru.perm.medicalservices.ordercrud.Entity.EmployeeEntity">
            <column name="E_Login" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>