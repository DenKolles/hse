<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="ru.perm.medicalservices.ordercrud.Entity.PostEntity" table="Post" schema="public" catalog="postgres">
        <id name="idPost">
            <column name="ID_Post" sql-type="integer"/>
        </id>
        <property name="pName">
            <column name="P_Name" sql-type="text"/>
        </property>
        <set name="employeesByIdPost" inverse="true">
            <key>
                <column name="ID_Post" not-null="true"/>
            </key>
            <one-to-many not-found="ignore" class="ru.perm.medicalservices.ordercrud.Entity.EmployeeEntity"/>
        </set>
    </class>
</hibernate-mapping>